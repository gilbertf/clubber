        "abc.de",
        "127.0.0.1",
]
SECRET_KEY: "xxx"

# Mail: User notification on event changes
EMAIL_NOTIFICATION_ENABLE: True
EMAIL_NOTIFICATION_NEW_EVENT_DEFAULT: True
EMAIL_NOTIFICATION_JOINED_EVENT_DEFAULT: True
EMAIL_HOST: "smtp.xxx.de"
EMAIL_PORT: 465
EMAIL_USE_TLS: False
EMAIL_USE_SSL: True
EMAIL_HOST_USER: "a"
EMAIL_HOST_PASSWORD: "xxx"
DEFAULT_FROM_EMAIL: "a@xxx.de"
EMAIL_SITE_URL: "https://www.abc.de"
